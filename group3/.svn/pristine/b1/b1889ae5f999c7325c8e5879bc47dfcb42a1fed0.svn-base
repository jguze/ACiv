package civ;

import civ.UI.MiniMapView;
import civ.engine.GameEngine;
import java.io.Serializable;

/**
 * The object to be saved in a file. Add game objects
 * that we want to save to this object
 * @author JustinIsAwesome Guze
 */
public class SaveObject implements Serializable {
    /**
     * The game engine to be saved
     */
    public GameEngine gameEngine;
        
    /**
     * The gameEngine to save to the file
     * @param gameEngine
     */
    public SaveObject(GameEngine gameEngine) {
        this.gameEngine = gameEngine;
    }
}
